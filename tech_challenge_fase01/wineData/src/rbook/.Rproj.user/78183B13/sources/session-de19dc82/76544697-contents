names <- c()

for ( name in colnames(ts_tbl)){
    if (name == 'date'){
        name = "Dia/Mês/Ano"
    } else if( name == 'volume'){
        name = "Quilograma Líquido"
    } else if( name == 'value'){
        name = "Valor FOB (US$)"
    } else{
        name = "País"
    }
        
    names <- c(names, name)
    
}